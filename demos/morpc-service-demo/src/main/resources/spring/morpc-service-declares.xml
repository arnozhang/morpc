       xmlns:morpc="http://schema.morpc.com/morpc/schema"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
         http://schema.morpc.com/morpc/schema http://schema.morpc.com/morpc/schema-1-0-0.xsd"
       default-autowire="byName">

    <bean id="demoBeansListService"
          class="com.morpc.demo.service.declare.list.DemoBeansListServiceImpl"/>

    <morpc:service
